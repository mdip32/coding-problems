already_seen <- function(input) {
i <- 1
while (TRUE) {
v_sum <- cumsum(rep(input, i))
has_dup <- any(duplicated(v_sum))
if (has_dup) {
return(v_sum[which(duplicated(v_sum))[1]])
}
i <- i + 1
}
}
